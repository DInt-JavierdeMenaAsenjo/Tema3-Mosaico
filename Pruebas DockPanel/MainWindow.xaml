<Window x:Class="Pruebas_DockPanel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pruebas_DockPanel"
        mc:Ignorable="d"
        Icon="assets/seychelles.ico"
        Title="MainWindow" Height="600" Width="600" MinHeight="470" MinWidth="450" >
    <DockPanel>
        <TextBlock DockPanel.Dock="Top" Height="100" Background="Red"></TextBlock>
        <TextBlock DockPanel.Dock="Left" Width="100" Background="red"></TextBlock>
        <TextBlock DockPanel.Dock="Bottom" Height="100" Background="Blue"></TextBlock>
        <TextBlock DockPanel.Dock="Left" Width="100" Background="Green"></TextBlock>
        
        <TextBlock DockPanel.Dock="Right" Width="100" Background="BLUE"></TextBlock>
        <TextBlock DockPanel.Dock="Top" Height="100" Background="Purple"></TextBlock>

        <TextBlock DockPanel.Dock="Bottom" Height="100" Background="Green"></TextBlock>
        <TextBlock DockPanel.Dock="Right" Width="100" Background="Purple"></TextBlock>
        <TextBlock Background="Black"></TextBlock>
    </DockPanel>
</Window>
